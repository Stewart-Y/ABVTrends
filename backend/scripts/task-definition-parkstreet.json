{
    "family": "abvtrends-prod-backend",
    "taskRoleArn": "arn:aws:iam::911167925148:role/abvtrends-prod-ecs-task",
    "executionRoleArn": "arn:aws:iam::911167925148:role/abvtrends-prod-ecs-exec",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "backend",
            "image": "911167925148.dkr.ecr.us-east-1.amazonaws.com/abvtrends-prod-backend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "DEMO_MODE",
                    "value": "false"
                },
                {
                    "name": "DEPLOY_TIMESTAMP",
                    "value": "2025-12-14-v27-security-hardening"
                },
                {
                    "name": "DEBUG",
                    "value": "false"
                },
                {
                    "name": "ENVIRONMENT",
                    "value": "production"
                },
                {
                    "name": "DB_SSL_REQUIRED",
                    "value": "true"
                },
                {
                    "name": "RUN_MIGRATIONS",
                    "value": "true"
                },
                {
                    "name": "LIBDIB_ENTITY_SLUG",
                    "value": "vista-wine-and-spirits-santee"
                },
                {
                    "name": "SGWS_ACCOUNT_ID",
                    "value": "102376"
                },
                {
                    "name": "RNDC_ACCOUNT_ID",
                    "value": "601009207"
                },
                {
                    "name": "SCRAPER_ITEMS_PER_RUN",
                    "value": "50"
                },
                {
                    "name": "SCRAPER_CATCHUP_ITEMS_PER_RUN",
                    "value": "100"
                },
                {
                    "name": "SCRAPER_NEW_ITEMS_RATIO",
                    "value": "0.8"
                },
                {
                    "name": "SCRAPER_REFRESH_ITEMS_RATIO",
                    "value": "0.2"
                },
                {
                    "name": "SCRAPER_MORNING_WINDOW_START",
                    "value": "7"
                },
                {
                    "name": "SCRAPER_MORNING_WINDOW_END",
                    "value": "11"
                },
                {
                    "name": "SCRAPER_EVENING_WINDOW_START",
                    "value": "19"
                },
                {
                    "name": "SCRAPER_EVENING_WINDOW_END",
                    "value": "23"
                },
                {
                    "name": "SCRAPER_MAX_JITTER_MINUTES",
                    "value": "60"
                }
            ],
            "secrets": [
                {
                    "name": "DATABASE_URL",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/database-url"
                },
                {
                    "name": "OPENAI_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/api-keys-Lat3Kr:OPENAI_API_KEY::"
                },
                {
                    "name": "PARKSTREET_EMAIL",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:PARKSTREET_EMAIL::"
                },
                {
                    "name": "PARKSTREET_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:PARKSTREET_PASSWORD::"
                },
                {
                    "name": "BREAKTHRU_EMAIL",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:BREAKTHRU_EMAIL::"
                },
                {
                    "name": "BREAKTHRU_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:BREAKTHRU_PASSWORD::"
                },
                {
                    "name": "LIBDIB_EMAIL",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:LIBDIB_EMAIL::"
                },
                {
                    "name": "LIBDIB_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:LIBDIB_PASSWORD::"
                },
                {
                    "name": "SGWS_EMAIL",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:SGWS_EMAIL::"
                },
                {
                    "name": "SGWS_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:SGWS_PASSWORD::"
                },
                {
                    "name": "RNDC_EMAIL",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:RNDC_EMAIL::"
                },
                {
                    "name": "RNDC_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:RNDC_PASSWORD::"
                },
                {
                    "name": "SIPMARKET_EMAIL",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:SIPMARKET_EMAIL::"
                },
                {
                    "name": "SIPMARKET_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:911167925148:secret:abvtrends/prod/distributor-credentials:SIPMARKET_PASSWORD::"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/abvtrends-prod/backend",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
}
